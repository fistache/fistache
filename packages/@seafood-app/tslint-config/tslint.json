{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "no-console": false,
    "no-empty-interface": false,
    "no-empty": false,
    "object-literal-sort-keys": false,
    "interface-name": [true, "never-prefix"],
    "semicolon": [true, "never"],
    "quotemark": [true, "single"],
    "trailing-comma": [true, {
      "multiline": "never",
      "singleline": "never"
    }]
  }
}
