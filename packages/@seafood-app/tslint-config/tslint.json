{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "no-console": false,
    "no-empty-interface": false,
    "object-literal-sort-keys": false,
    "interface-name": false,
    "no-empty": false
  },
  "jsRules": {
    "max-line-length": {
      "options": [120]
    }
  }
}
